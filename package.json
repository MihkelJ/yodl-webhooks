{
  "name": "yodl-webhooks",
  "version": "0.1.0",
  "main": "public/server.js",
  "type": "module",
  "scripts": {
    "start": "node public/server.js",
    "dev": "nodemon src/server.ts -e ts --exec tsx",
    "build": "prisma generate && tsc && tsc-alias",
    "watch": "tsc -w",
    "postinstall": "prisma generate"
