    "notifications"
  ],
  "author": "MihkelJ",
  "license": "MIT",
  "description": "Yodl Webhooks",
  "dependencies": {
    "@prisma/client": "6.2.1",
    "@types/express": "^5.0.0",
    "@types/http-errors": "^2.0.4",
    "@types/node": "^22.10.10",
    "@types/web-push": "^3.6.4",
    "express": "^4.21.2",
    "express-zod-api": "^22.1.1",
    "http-errors": "^2.0.0",
    "prisma": "^6.2.1",
    "typescript": "^5.7.3",
    "viem": "^2.22.14",
    "web-push": "^3.6.7",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "nodemon": "^3.1.9",
    "tsx": "^4.19.2",
